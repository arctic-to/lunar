{
  "name": "lunar",
  "version": "0.1.0",
  "private": true,
  "main": "build/main.js",
  "files": [
    "/build/src",
    "!/build/src/**/*.map"
  ],
  "scripts": {
    "--------------------------Dev Scripts------------------------": "",
    "dev:renderer": "next dev",
    "watch:main": "tsc -w src/main.ts --esModuleInterop --outDir build",
    "dev:main": "wait-on http://localhost:3000 && nodemon --watch ./build --exec electron .",
    "predev": "rimraf ./build",
    "dev": "concurrently yarn:watch:main yarn:dev:*",
    "--------------------------Dist Scripts------------------------": "",
    "dist": "yarn run build && electron-builder",
    "pack": "yarn run build && electron-builder --dir",
    "--------------------------Misc Scripts------------------------": "",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "--------------------------Build Scripts-----------------------": "",
    "build:renderer": "next build && next export -o build",
    "build:main": "tsc src/main.ts --esModuleInterop --outDir build",
    "build": "yarn run build:renderer && yarn run build:main",
    "--------------------------Prod Scripts (Unused)---------------": "",
    "start:renderer": "next start",
    "start:main": "wait-on http://localhost:3000 && electron .",
    "start": "concurrently yarn:start:*"
  },
  "dependencies": {
    "@hookform/resolvers": "^1.3.0",
    "@popperjs/core": "^2.9.2",
    "@prisma/client": "^2.24.1",
    "@types/classnames": "^2.2.11",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.8",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "lodash": "^4.17.20",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "mobx-state-tree": "^4.0.2",
    "next": "^10.2.3",
    "qs": "^6.9.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.13.1",
    "react-icons": "^4.1.0",
    "react-popper": "^2.2.5",
    "react-use": "^15.3.4",
    "sass": "^1.30.0",
    "swr": "^0.3.11",
    "unit-of-time": "^0.1.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.12",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^5.3.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.10.5",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "hitoka": "^0.6.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "prisma": "^2.24.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "wait-on": "^5.2.0"
  }
}
