{
  "name": "lunar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:renderer": "next dev",
    "build:renderer": "next build",
    "start:renderer": "next start",
    "watch:main": "tsc -w src/main.ts --outDir build",
    "dev:main": "wait-on http://localhost:3000 && nodemon --watch ./build --exec electron .",
    "predev": "rimraf ./build",
    "dev": "concurrently yarn:watch:main yarn:dev:*",
    "demo": "electron",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "main": "build/main.js",
  "files": [
    "/build/src",
    "!/build/src/**/*.map"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "electron": "^11.1.1",
    "eslint": "^7.15.0",
    "hitoka": "^0.6.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "wait-on": "^5.2.0"
  }
}
